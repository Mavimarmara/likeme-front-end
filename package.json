{
  "name": "likeme-front-end",
  "version": "1.0.0",
  "description": "React Native app for health and wellness",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "start:reset": "expo start --clear --reset-cache",
    "start:tunnel": "expo start --tunnel",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "android:clear": "expo start --clear --android",
    "ios": "expo run:ios",
    "ios:clear": "expo start --clear --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:e2e": "~/.maestro/bin/maestro test maestro/",
    "test:e2e:welcome": "~/.maestro/bin/maestro test maestro/welcome-screen.yaml",
    "test:e2e:objectives": "~/.maestro/bin/maestro test maestro/personal-objectives.yaml",
    "test:e2e:onboarding": "~/.maestro/bin/maestro test maestro/onboarding-flow.yaml",
    "test:e2e:journey": "~/.maestro/bin/maestro test maestro/complete-user-journey.yaml",
    "test:e2e:expo-go": "./maestro/run-expo-go-tests.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "doctor": "expo doctor",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "build:android": "eas build --platform android",
    "build:android:local": "./build-android-local.sh",
    "build:android:gradle": "npx expo prebuild --platform android && cd android && ./gradlew assembleRelease && cd ..",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "run:apk": "./run-apk.sh",
    "run:emulator": "./start-emulator.sh",
    "install:apk": "./install-and-run-apk.sh",
    "clean": "rm -rf node_modules/.cache .expo dist android/build ios/build",
    "clean:all": "rm -rf node_modules/.cache .expo dist android/build ios/build android/app/build ios/Pods ios/Podfile.lock",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf Pods Podfile.lock build && pod install && cd ..",
    "postinstall": "if [ \"$SKIP_IOS_POD_INSTALL\" != \"true\" ]; then cd ios && pod install && cd ..; fi"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "54.0.22",
    "expo-asset": "^12.0.9",
    "expo-auth-session": "^7.0.8",
    "expo-blur": "~15.0.7",
    "expo-constants": "^18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "~6.0.17",
    "expo-file-system": "^19.0.17",
    "expo-font": "~14.0.9",
    "expo-linear-gradient": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-auth0": "^5.1.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.11.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-vector-icons": "^10.0.2",
    "react-native-web": "^0.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.81.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/react-native": "^13.3.3",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "~19.1.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv": "^17.2.3",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "prettier": "^2.4.1",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-vector-icons|expo|@expo|expo-auth-session|@react-navigation|@react-native-community)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@/assets/(.*)$": "<rootDir>/assets/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "overrides": {
    "react-native-appwrite": {
      "expo-file-system": "18.*.*",
      "react-native": ">=0.81.0 <1.0.0"
    }
  }
}
