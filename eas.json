{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "staging": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://likeme-back-end-git-staging-pixel-pulse-labs.vercel.app",
        "EXPO_PUBLIC_AUTH0_DOMAIN": "likeme.us.auth0.com",
        "EXPO_PUBLIC_USE_AUTH_PROXY": "true",
        "EXPO_PUBLIC_AUTH_SCHEME": "likeme",
        "EXPO_PUBLIC_AUTH_PROXY_URL": "https://auth.expo.dev/@pixelpulselab/likeme-front-end",
        "EXPO_PUBLIC_AUTH_PROXY_PROJECT": "@pixelpulselab/likeme-front-end",
        "EXPO_PUBLIC_AUTH_REDIRECT_PATH": "auth",
        "EXPO_USE_HERMES": "true",
        "SKIP_IOS_POD_INSTALL": "true"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk",
        "env": {
          "SKIP_IOS_POD_INSTALL": "true"
        }
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://likeme-back-end-one.vercel.app",
        "EXPO_PUBLIC_AUTH0_DOMAIN": "likeme.us.auth0.com",
        "EXPO_PUBLIC_USE_AUTH_PROXY": "true",
        "EXPO_PUBLIC_AUTH_SCHEME": "likeme",
        "EXPO_PUBLIC_AUTH_PROXY_URL": "https://auth.expo.dev/@pixelpulselab/likeme-front-end",
        "EXPO_PUBLIC_AUTH_PROXY_PROJECT": "@pixelpulselab/likeme-front-end",
        "EXPO_PUBLIC_AUTH_REDIRECT_PATH": "auth",
        "EXPO_USE_HERMES": "true",
        "SKIP_IOS_POD_INSTALL": "true"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_USE_HERMES": "true",
          "SKIP_IOS_POD_INSTALL": "true"
        },
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "production-apk": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://likeme-back-end-one.vercel.app",
        "EXPO_PUBLIC_AUTH0_DOMAIN": "likeme.us.auth0.com",
        "EXPO_PUBLIC_USE_AUTH_PROXY": "true",
        "EXPO_PUBLIC_AUTH_SCHEME": "likeme",
        "EXPO_PUBLIC_AUTH_PROXY_URL": "https://auth.expo.dev/@pixelpulselab/likeme-front-end",
        "EXPO_PUBLIC_AUTH_PROXY_PROJECT": "@pixelpulselab/likeme-front-end",
        "EXPO_PUBLIC_AUTH_REDIRECT_PATH": "auth",
        "EXPO_USE_HERMES": "true",
        "SKIP_IOS_POD_INSTALL": "true"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_USE_HERMES": "true",
          "SKIP_IOS_POD_INSTALL": "true"
        }
      }
    }
  },
  "submit": {
